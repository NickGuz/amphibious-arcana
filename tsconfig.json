{
    "include": [
        "src/**/*",
        "src/*"
    ],
    "compilerOptions": {
        "noImplicitAny": true,
        "target": "es2016",
        "module": "commonjs",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    }
}